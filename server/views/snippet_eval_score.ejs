<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
<% var i = 0; %>
<% for (var index in data) {%>
<% var trip = data[index].trip; %>
  
  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="heading-<%=trip.id%>">
      <h4 class="panel-title">
       <% if (i == 0) { %>
        <a class="header-color-yellow" data-toggle="collapse" data-parent="#accordion" href="#collapse-<%=trip.id%>" aria-expanded="true" aria-controls="collapse-<%=trip.id%>">
       <%  } else { %>
        <a class="header-color-yellow"  data-toggle="collapse" data-parent="#accordion" href="#collapse-<%=trip.id%>" aria-expanded="false" aria-controls="collapse-<%=trip.id%>"> 
       <%  } %>
        <%=trip.name%>
        </a>
      </h4>
    </div>
    <% if (i == 0) { %>
        <div id="collapse-<%=trip.id%>" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading-<%=trip.id%>">
    <%  } else { %>  
        <div id="collapse-<%=trip.id%>" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading-<%=trip.id%>">
    <%  } %>  
    <div class="panel-body">
      <div class="container col-md-offset-1">
        <% for (var index in trip.questions) {%>
        <% var question = trip.questions[index][0]; %>
         <% if (index == 0) { %>
         <div class="row col-md-8 container-chart">

              <h4 class="main-text-header header-score">Allgemein</h4>
              <span class="col-md-1 glyphicon glyphicon glyphicon-user"></span>
              <div class="col-md-11 t-score">
              
               <div> Insgesamt haben <b id="info-sum-participants">x</b> Personen den Fragebogen bearbeitet</div> 
               <div><b id="info-sum-participants-finish">x</b> Reisende haben den Fragebogen vollständig beantwortet</div>
               <div><b id="info-sum-participants-cancel">x</b> Reisende haben den Fragebogen vorzeitig beendet</div> 
                <br/>
            </div>
           <span class="col-md-1 glyphicon glyphicon glyphicon-time"></span>
           <div class="col-md-11 t-score">
              <div>Für die Beantwortung des Fragebogens haben die Kunden durchschnittlich <b id="info-avg-time">0</b> Sekunden benötigt</div> 
            </div>            

         </div>
         <%  } %>  
         <div class="row col-md-8 container-chart">
         <h4 class="main-text-header header-score">Auswertung: <%=question.text%></h4>
         <% if (question.type == 0) { %>
              <canvas class="chart chart-<%=question.question_id%>" id="chart-<%=question.question_id%>" data-trip-id="<%=trip.id%>" data-question-id="<%=question.question_id%>"></canvas>
          <%  } %>
         <% if (question.type == 1) { %>
          <table class="chart" id="chart-<%=question.question_id%>" data-trip-id="<%=trip.id%>" data-question-id="<%=question.question_id%>">            
          </table>
        <%  } %>
        <% if (question.type == 3) { %>
          <div class="chart chart-<%=question.question_id%>" data-trip-id="<%=trip.id%>" data-question-id="<%=question.question_id%>"></div>
         <%  } %>
         </div>    
        <%  } %>
      </div>
    </div>
  </div>
<% i++ %>
<% } %>  
</div>
   	
