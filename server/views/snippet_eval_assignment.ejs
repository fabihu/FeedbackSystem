<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
<% var i = 0; %>
<% for (var index in trips) {%>
<% var trip = trips[index]; %>
<% var questionset = questions[index]; %> 

  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="heading-<%=trip.id%>">
      <h4 class="panel-title">
       <% if (i == 0) { %>
        <a data-toggle="collapse" data-parent="#accordion" href="#collapse-<%=trip.id%>" aria-expanded="true" aria-controls="collapse-<%=trip.id%>">
       <%  } else { %>
        <a data-toggle="collapse" data-parent="#accordion" href="#collapse-<%=trip.id%>" aria-expanded="false" aria-controls="collapse-<%=trip.id%>"> 
       <%  } %>
        <%=trip.name%>
        </a>
      </h4>
    </div>
    <% if (i == 0) { %>
        <div id="collapse-<%=trip.id%>" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading-<%=trip.id%>">
    <%  } else { %>  
        <div id="collapse-<%=trip.id%>" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading-<%=trip.id%>">
    <%  } %>  
      <div class="panel-body">
        <div class="container">
    <br />
    <div class="row">

        <div class="dual-list list-left col-md-5">
            <div class="well text-right">
                <div class="row">
                    <div class="col-md-10">
                        <div class="input-group">
                            <span class="input-group-addon glyphicon glyphicon-search"></span>
                            <input type="text" name="SearchDualList" class="form-control" placeholder="Suche..." />
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="btn-group">
                            <a class="btn btn-default selector" title="select all"><i class="glyphicon glyphicon-unchecked"></i></a>
                        </div>
                    </div>
                </div>
                <ul class="list-group">

      
    <% for (var j = 0; j<questionset.length; j++) {%>
    <% var questionrow = questionset[j]; %>
        <% for (var second_index in questionrow) {%>
        <% var question = questionrow[second_index]; %>                    
                  <li class="list-group-item"><%=question.text%></li>
        <%  } %>  
    <%  } %>     
                  

                </ul>
            </div>
        </div>

        <div class="list-arrows col-md-1 text-center">
            <button class="btn btn-default btn-sm move-left">
                <span class="glyphicon glyphicon-chevron-left"></span>
            </button>

            <button class="btn btn-default btn-sm move-right">
                <span class="glyphicon glyphicon-chevron-right"></span>
            </button>
        </div>

        <div class="dual-list list-right col-md-5">
            <div class="well">
                <div class="row">
                    <div class="col-md-2">
                        <div class="btn-group">
                            <a class="btn btn-default selector" title="select all"><i class="glyphicon glyphicon-unchecked"></i></a>
                        </div>
                    </div>
                    <div class="col-md-10">
                        <div class="input-group">
                            <input type="text" name="SearchDualList" class="form-control" placeholder="Suche..." />
                            <span class="input-group-addon glyphicon glyphicon-search"></span>
                        </div>
                    </div>
                </div>
                <ul class="list-group">  
                 
              <% for (var k = 0; k<active_questions.length; k++) {%>
                <% var active_question = active_questions[k]; %>               
                       
                         <li class="list-group-item"><%=active_question.text%></li>
                    
                  <%  } %>
                </ul>
            </div>
        </div>

    </div>
</div>
      </div>
    </div>
  </div>
<% i++ %>
<% } %>  
</div>
   	
