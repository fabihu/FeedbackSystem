 <h2>Fragen verwalten</h2>
 <div class="col-md-10 container">
    <div class="row col-md-10 custyle">
    <table class="table table-striped custab">
    <thead>   
        <tr>
            <th>ID</th>
            <th>Frage</th>
            <th>Typ</th>
            <th class="text-center">Action</th>
        </tr>
    </thead>
    	<% for (var index in questions) {%>
    	<% var question = questions[index]; %>
            <tr id="question-row-<%=question.id%>" data-question-id="<%=question.id%>">
                <td><%=question.id%></td>
                <td><%=question.question%></td>
                <td><%=question.type%> </td>
                <td class="text-center">
                    <a href="#" class='btn btn-info btn-xs'><span class="glyphicon glyphicon-edit"></span></a>
                    <a href="#" class="btn btn-danger btn-xs"><span class="glyphicon glyphicon-remove"></span></a>
                    <a href="#" class='btn btn-info btn-xs btn-detail' id="btn-detail-question-<%=question.id%>" data-question-id="<%=question.id%>" data-detail-active="false"><span class="glyphicon glyphicon-chevron-down"></span></a></td>
            </tr>
        <% } %>             
    </table>
    </div>
</div>  