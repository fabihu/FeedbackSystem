          <button class="btn-checkbox-dem btn-lg btn-gender" id="check-male" data-checked="false" data-val="m"><span class="glyphicon glyphicon-none"></span></button>
          <label class="question-answer-option col-md-4" for="check-male"> männlich </label>
        </div>

      <div  class="row"> 
         <button class="btn-checkbox-dem btn-lg btn-gender" id="check-female" data-checked="false" data-val="f"><span class="glyphicon glyphicon-none"></span> </button>                
         <label class="question-answer-option col-md-4" for="check-female"> weiblich </label>
      </div>

      <div  class="row"> 
        <button class="btn-checkbox-dem btn-lg btn-gender" id="check-na" data-checked="false" data-val="n"><span class="glyphicon glyphicon-none"></span></button>
        <label class="question-answer-option col-md-4" for="check-na"> keine Angaben </label>
      </div>
      <button class="btn-next-dem btn-next-gender" id="button-next-gender">Weiter ></button>
      </div>

       <div class="row col-md-12 container main-text demograph-age invis" id='container-age'>     
