         <button class="btn-checkbox-dem btn-lg btn-age" id="check-y-2" data-checked="false" data-val="2"><span class="glyphicon glyphicon-none"></span></button>
         <label class="question-answer-option col-md-4" for="check-y-2"> 30-39 </label>
        </div>
        <div  class="row"> 
         <button class="btn-checkbox-dem btn-lg btn-age" id="check-y-3" data-checked="false" data-val="3"><span class="glyphicon glyphicon-none"></span></button>
         <label class="question-answer-option col-md-4" for="check-y-3"> 40-49 </label>
        </div>
        <div  class="row"> 
         <button class="btn-checkbox-dem btn-lg btn-age" id="check-y-4" data-checked="false" data-val="4"><span class="glyphicon glyphicon-none"></span></button>
         <label class="question-answer-option col-md-4" for="check-y-4"> 50-59 </label>
        </div>
        <div  class="row"> 
         <button class="btn-checkbox-dem btn-lg btn-age" id="check-y-5" data-checked="false" data-val="5"><span class="glyphicon glyphicon-none"></span></button>
         <label class="question-answer-option col-md-4" for="check-y-5"> 60-69 </label>
